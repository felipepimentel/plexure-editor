{
  "name": "plexure-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:setup": "bash scripts/setup-db.sh",
    "db:reset": "PGPASSWORD=$SUPABASE_DB_PASSWORD supabase db reset",
    "db:push": "PGPASSWORD=$SUPABASE_DB_PASSWORD supabase db push",
    "db:seed": "for f in supabase/seed/*.sql; do PGPASSWORD=$SUPABASE_DB_PASSWORD psql $DATABASE_URL -f $f; done",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@supabase/supabase-js": "2.47.3",
    "jwt-decode": "4.0.0",
    "lucide-react": "0.468.0",
    "monaco-yaml": "^5.2.3",
    "openapi-types": "12.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-syntax-highlighter": "^15.6.1",
    "uuid": "11.0.3",
    "yaml": "2.6.1",
    "zod": "3.24.0",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@storybook/addon-essentials": "^8.4.7",
